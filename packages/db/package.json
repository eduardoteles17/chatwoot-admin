{
  "name": "@chatwoot-admin/db",
  "private": true,
  "exports": {
    ".": [
      "./src/index.ts"
    ],
    "./*": [
      "./src/*"
    ]
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate:new": "dotenvx run -f ../../.env -f .env -- prisma migrate dev --create-only",
    "db:migrate:deploy": "dotenvx run -f ../../.env -f .env -- prisma migrate deploy"
  },
  "devDependencies": {
    "@chatwoot-admin/tsconfig": "workspace:*"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "prisma": "^6.13.0"
  }
}
